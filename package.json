{
  "name": "getmyjab",
  "version": "2.0.0",
  "description": "Angular 12 application to get live covid-19 vaccination slots",
  "homepage": "https://getmyjab.firebaseapp.com",
  "keywords": [
    "angular",
    "rxjs",
    "universal",
    "vaccine",
    "covid-19"
  ],
  "author": {
    "name": "anirbmuk",
    "email": "anirbmuk@live.in"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anirbmuk/getmyjab.git"
  },
  "bugs": {
    "url": "https://github.com/anirbmuk/getmyjab/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "node dist/getmyjab/server/main.js",
    "start:heroku": "node dist/getmyjab/server/main.js",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open --browser chrome",
    "cy:test": "cypress run --headless --browser chrome",
    "build": "ng build",
    "build:debug": "ng build --configuration production --source-map",
    "explore": "source-map-explorer dist/getmyjab/**/*.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run getmyjab:serve-ssr",
    "serve:ssr": "node dist/getmyjab/server/main.js",
    "build:ssr": "ng build --configuration prodlike && ng run getmyjab:server:prodlike",
    "prerender": "ng run getmyjab:prerender",
    "format": "prettier --config ./.prettierrc --write \"src/app/**/*{.ts,.js,.json,.css,.scss,.html}\" \"src/styles.css\" \"src/index.html\" \"cypress/**/*{.js,.json}\"",
    "clean": "rimraf .firebase && rimraf dist",
    "deploy:ssr": "npm run build:ssr && gcloud config set project getmyjab && gcloud app deploy --quiet",
    "deploy:static": "ng build --configuration production && firebase deploy",
    "deploy:all": "npm run lint && npm run deploy:static && npm run clean && npm run deploy:ssr && npm run clean"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.3",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "~12.1.3",
    "@angular/compiler": "~12.1.3",
    "@angular/core": "~12.1.3",
    "@angular/forms": "~12.1.3",
    "@angular/material": "^12.1.3",
    "@angular/platform-browser": "~12.1.3",
    "@angular/platform-browser-dynamic": "~12.1.3",
    "@angular/platform-server": "~12.1.3",
    "@angular/router": "~12.1.3",
    "@angular/service-worker": "~12.1.3",
    "@ngneat/until-destroy": "^8.1.1",
    "@nguniversal/express-engine": "^12.1.0",
    "bootstrap": "^5.0.2",
    "express": "^4.15.2",
    "firebase": "^8.8.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.3",
    "@angular-eslint/builder": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/schematics": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "~12.1.3",
    "@angular/compiler-cli": "~12.1.3",
    "@babel/eslint-parser": "^7.14.4",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/experimental-utils": "^4.27.0",
    "@typescript-eslint/parser": "4.23.0",
    "cypress": "^7.5.0",
    "eslint": "^7.28.0",
    "firebase-tools": "^9.16.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  },
  "postinstall": "npm run build:ssr"
}
